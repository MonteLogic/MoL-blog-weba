{
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "config": {
    "contentRepo": "https://github.com/MonteLogic/MoL-blog-content.git",
    "author": "MonteLogic",
    "niceNameOfProject": "Mo99 Blog",
    "skillsGistUrl": "https://gist.githubusercontent.com/YOUR_USERNAME/GIST_ID/raw/skills-employability.json"
  },
  "scripts": {
    "setup": "ln -sf ../MoL-blog-content MoL-blog-content",
    "sync-content": "rm -rf MoL-blog-content && cp -r ../MoL-blog-content MoL-blog-content",
    "postinstall": "if [ ! -d MoL-blog-content ]; then git clone --depth 1 \"${CONTENT_REPO_URL:-$npm_package_config_contentRepo}\" MoL-blog-content; fi",
    "predev": "pnpm run generate-markdown-paths",
    "dev": "next dev",
    "lint": "biome check .",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "start": "next start",
    "vitest": "vitest",
    "test:db": "tsx tests/sqlite-test/setup-test-db.ts",
    "test:playwright": " exec playwright test",
    "local-db-create": "node tests/db-create/index.ts",
    "generate-markdown-paths": "ts-node -O '{\"module\": \"CommonJS\"}' scripts/generate.ts",
    "validate-pain-points": "ts-node -O '{\"module\": \"CommonJS\"}' scripts/validate-pain-points.ts",
    "check-categories": "node scripts/check-categories.js",
    "build": "pnpm run generate-markdown-paths && pnpm run validate-pain-points && pnpm run check-categories && pnpm run lint:strict && next build",
    "type-check": "tsc --noEmit --pretty",
    "type-check:watch": "tsc --noEmit --watch --pretty",
    "tsgo:check": "tsgo --noEmit",
    "tsgo:check:watch": "tsgo --noEmit --watch",
    "lint:strict": "biome check --reporter=github .",
    "lint:watch": "chokidar \"**/*.{ts,tsx,js,jsx}\" -i \"node_modules\" -i \".next\" -i \"MoL-blog-content\" -c \"biome check --reporter=github .\" --initial",
    "lint:fix": "biome check --write .",
    "lint:problems": "node scripts/biome-report.js --max 2000"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
